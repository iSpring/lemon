{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "app",
    "views"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "echo server restarted"
  },
  "watch": [
    "controllers/",
    "middlewares/",
    "models/",
    "config.js",
    "router.js",
    "server.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}